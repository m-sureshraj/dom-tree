{
  "name": "dom-tree",
  "version": "0.0.1",
  "description": "Convert JavaScript Object, Array, and JSON into HTML tree view",
  "main": "dist/js/dom_tree.min.js",
  "style": "dist/css/dom_tree.min.css",
  "scripts": {
    "prestart": "rm -rf ./dev/* && if [ ! -f './src/dom_tree.html' ]; then cp ./src/dom_tree_template.html ./src/dom_tree.html; fi",
    "start": "webpack-dev-server --config ./webpack/webpack.dev.js --env.script=start",
    "build:dev": "rm -rf ./dev/* && webpack --config ./webpack/webpack.dev.js --env.script=dev",
    "build:production": "rm -rf dist && mkdir dist && webpack --config ./webpack/webpack.prod.js",
    "lint": "run-p lint-js lint-css",
    "lint-js": "eslint '**/*.js' '.*.js'",
    "lint-js-fix": "eslint --fix",
    "lint-css": "stylelint './src/css/**/*.css'",
    "lint-css-fix": "stylelint --fix",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --collectCoverage",
    "report-coverage": "codecov",
    "prettier": "prettier --config ./prettier.config.js --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "run-s lint test:ci"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint-js-fix"
    ],
    "src/css/*.css": [
      "npm run lint-css-fix"
    ]
  },
  "author": "M.Sureshraj <m.s.suresh100@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/m-sureshraj/dom-tree.git"
  },
  "license": "MIT",
  "keywords": [
    "object tree",
    "array tree",
    "html tree view",
    "json to html tree",
    "convert javascript object to html tree view",
    "convert javascript array to html tree view"
  ],
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "codecov": "^3.1.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-prettier": "^2.7.0",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^23.6.0",
    "lint-staged": "^10.2.2",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-reporter": "^5.0.0",
    "prettier": "^1.15.3",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  }
}
